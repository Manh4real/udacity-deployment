    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.3",
    "pg": "^8.12.0",
    "rimraf": "^5.0.7",
    "typescript": "^4.1.3"
  },
