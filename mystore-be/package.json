  "main": "server.ts",
  "scripts": {
    "start": "npm run clear && npm install && node dist/server.js",
    "dev": "npm run build && nodemon dist/server.js",
    "build": "npm run clean && tsc && cp Procfile package.json dist",
    "watch": "npm run clean && tsc --watch",
    "clean": "rimraf dist/",
    "clear": "npm cache clean -f && rm -rf node_modules package-lock.json",
    "test": "ENV=test db-migrate --env test up && (jasmine || true) && db-migrate --env test reset"
  },
